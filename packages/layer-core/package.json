{
  "name": "@nuxt-layers-monorepo/layer-core",
  "type": "module",
  "version": "1.0.0",
  "main": "./nuxt.config.ts",
  "types": "./shared/types",
  "scripts": {
    "dev": "nuxt dev .playground",
    "dev:prepare": "nuxt prepare .playground",
    "postinstall": "pnpm dev:prepare",
    "build": "nuxt build .playground",
    "generate": "nuxt generate .playground",
    "preview": "nuxt preview .playground",
    "typecheck": "nuxt typecheck",
    "lint:script": "eslint .",
    "lint:script-fix": "eslint --fix .",
    "lint:style": "stylelint \"**/*.{css,vue}\"",
    "lint:style-fix": "pnpm lint:style --fix",
    "lint": "pnpm lint:script && pnpm lint:style",
    "lint-fix": "pnpm lint:script-fix && pnpm lint:style-fix",
    "test": "vitest run",
    "test:unit": "vitest run tests/unit",
    "test:coverage": "vitest run --coverage"
