    "nuxt": "catalog:",
    "nuxt-auth-utils": "catalog:",
    "nuxt-security": "catalog:",
    "pinia": "catalog:",
    "playwright-core": "catalog:",
    "stylelint": "catalog:",
    "stylelint-config-standard": "catalog:",
    "stylelint-config-standard-vue": "catalog:",
    "typescript": "catalog:",
    "vitest": "catalog:",
    "vue": "catalog:",
    "vue-router": "catalog:",
    "vue-tsc": "catalog:",
    "zod": "catalog:"
